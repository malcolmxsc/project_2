<%- include('../partials/header') %>

<h2>User Details</h2>
<p><strong>Username:</strong> <%= user.username %></p>

<h2>User Logs</h2>
<% if (user.log.length === 0) { %>
  <p>No logs found.</p>
<% } else { %>
  <% user.log.forEach(logEntry => { %>
    <div>
      <h3>Exercise Date: <%= logEntry.date %></h3>
      <p>Exercise Type: <%= logEntry.exerciseType %></p>
      <p>Exercise Notes: <%= logEntry.exerciseNotes %></p>
      <p>Exercise Time (minutes): <%= logEntry.exerciseTime %></p>
      <!-- Add any other log properties you want to display -->
    </div>
    <hr>
  <% }); %>
<% } %>

<%- include('../partials/footer') %>
